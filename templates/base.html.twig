        <div class="container body-container">
            {% block body %}
                <div class="row">
				                    <div id="main" class="col-12 col-md-2 p-3 p-md-5 text-center">
									<a href="https://mesinkasironline.web.app" target="blank">
                   <img class="img-fluid" width="60" alt="symfony new blog platform"  src="{{ asset('build/images/symfony-logo-AA34C8FC16-seeklogo.com (1).png') }}"/>
				</a>
				<a href="https://fb.com/mesinkasircomplete" target="blank">
                <img class="img-fluid" width="60" alt="symfony new blog platform"  src="{{ asset('build/images/fb.svg') }}"/>
				</a>
				<a href="https://mesinkasir.github.io" target="blank">
                <img class="img-fluid" width="60" alt="symfony new blog platform"  src="{{ asset('build/images/git.svg') }}"/>
				</a>
				<a href="https://www.youtube.com/c/bakoelraksupermarketMesinKasir/videos" target="blank">
                <img class="img-fluid" width="60" alt="symfony new blog platform"  src="{{ asset('build/images/yt.svg') }}"/>
								  </a>
								  <a href="https://twitter.com/hockeycomp" target="blank">
                <img class="img-fluid" width="60" alt="symfony new blog platform"  src="{{ asset('build/images/tw.png') }}"/>
								  </a>
					</div>
                    <div id="main" class="col-12 col-md-7 p-3 p-md-5">
                        {{ include('default/_flash_messages.html.twig') }}

                        {% block main %}{% endblock %}
                    </div>

                    <div id="sidebar" class="col-12 col-md-3 p-3">
                        {% block sidebar %}
                            {{ render_esi(controller('Symfony\\Bundle\\FrameworkBundle\\Controller\\TemplateController::templateAction', {
                                'template': 'blog/about.html.twig',
                                'sharedAge': 600,
                                '_locale': app.request.locale
                            })) }}
                        {% endblock %}
                    </div>
                </div>
            {% endblock %}
        </div>

        {% block footer %}
            <footer>
                <div class="container">
                    <div class="row">
                        <div id="footer-copyright" class="col-md-12 text-center">
